{
  "version": 2,
  "builds": [
    {
      "src": "html-pdf-backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/html-pdf-backend/server.js"
    }
  ],
  "functions": {
    "html-pdf-backend/server.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
} 